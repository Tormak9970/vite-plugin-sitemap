{
  "name": "@tormak/vite-plugin-sitemap",
  "version": "1.1.18",
  "description": "Automatic Sitemap generation for Vite projects, based on provided routes",
  "type": "module",

  "files": [
    "dist",
    "src",
    "exports.js",
    "index.d.ts"
  ],

  "main": "./dist/vite-plugin-sitemap.umd.js",
  "module": "./dist/vite-plugin-sitemap.es.js",
  "types": "./index.d.ts",

  "exports": {
    ".": {
      "import": "./dist/vite-plugin-sitemap.es.js",
      "require": "./dist/vite-plugin-sitemap.umd.js",
      "types": "./index.d.ts"      
    }
  },

  "dependencies": {
    "vite": "^2.9.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.23"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --build --clean && vite build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tormak9970/vite-plugin-sitemap.git"
  },
  "keywords": [
    "vite-plugin",
    "sitemap",
    "extendable"
  ],
  "author": {
    "name": "Travis Lane",
    "email": "Tormak9970@gmail.com",
    "url": "https://travislane.dev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tormak9970/vite-plugin-sitemap/issues"
  },
  "homepage": "https://github.com/Tormak9970/vite-plugin-sitemap"
}
